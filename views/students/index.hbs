{{#extends "base"}}
{{#block "content"}}
<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        {{#each students}}
        <tr>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.gender}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>

{{/block}}
{{/extends}}